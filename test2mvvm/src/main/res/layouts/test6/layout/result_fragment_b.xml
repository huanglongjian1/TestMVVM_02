<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="dataab"
            type="com.android.test2mvvm.test6.fragments.result.ViewModel_AB" />

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/result_fragment_b_tv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@{dataab.stringMutableLiveData}"
            android:textSize="30sp" />

        <TextView
            android:id="@+id/result_fragment_b_tv_02"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="result_fragment_b_tv_02"
            android:textSize="30sp" />

        <TextView
            android:id="@+id/result_fragment_b_tv_eventbus"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="eventbus测试"
            android:textSize="30sp" />

        <androidx.appcompat.widget.AppCompatSeekBar
            android:id="@+id/result_fragment_b_seekbar"
            android:progressDrawable="@drawable/sb_bar"
            android:thumb="@drawable/sb_thumb"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </LinearLayout>
</layout>