<?xml version="1.0" encoding="UTF-8" ?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/test6_fragment_linearlayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ProgressBar
            android:id="@+id/test6_fragment_progress"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="20sp" />

        <Button
            android:id="@+id/test6_fragment_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="点击"
            android:textColor="@color/black"
            android:textSize="20sp" />

        <Button
            android:id="@+id/test6_fragment_btn2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="点击2"
            android:textColor="@color/black"
            android:textSize="20sp" />
        <Button
            android:id="@+id/test6_fragment_btn3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="点击3"
            android:textColor="@color/black"
            android:textSize="20sp" />

        <WebView
            android:id="@+id/test6_fragment_webview"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" />
    </androidx.appcompat.widget.LinearLayoutCompat>
</layout>